{
  "name": "react-static-example-typescript",
  "private": true,
  "scripts": {
    "start": "npm run build:styles && react-static start",
    "stage": "react-static build --staging",
    "build": "npm run build:styles && react-static build --debug",
    "build:styles": "tailwindcss build base.styles.css -o src/styles.css",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000 -s"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.19.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-static": "^7.2.0",
    "react-static-plugin-reach-router": "^7.2.0",
    "react-static-plugin-sitemap": "^7.2.0",
    "react-static-plugin-source-filesystem": "^7.2.0",
    "react-static-plugin-typescript": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "@types/reach__router": "^1.2",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack-env": "^1.14.0",
    "react-hot-loader": "^4.12.11",
    "serve": "^11.1.0",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.5.3"
  }
}
